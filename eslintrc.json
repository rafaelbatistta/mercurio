{
    "extends": ["@rocketseat/eslint-config/node"],
    "rules": {
      "quotes": ["error", "double"],
      "semi": ["error", "always"],
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto",
          "semi": true
        }
      ],
      "no-useless-constructor": "off"
    }
  }
  